    jQuery('#explore-strava, #challenge-list-view, .promo-simple, .promo-fancy, .promo-overlay, .sponsor-link-section, .sponsor').on('click', 'a', function(event) {
      var link = jQuery(event.target).closest('a');
      var adzerkClickUri = link.data('adzerk-click-uri');
      if (adzerkClickUri != null) {
        jQuery.get(adzerkClickUri); // this is fire-and-forget - we don't need to wait for a successful response from Adzerk
      }
    });
  });
</script>

<script>
  jQuery(document).ready(function() {
    var shareController = new Strava.Share.Controller.getInstance(null, {
      source: 'new-activity',
      activity_id: 3581188761
    });
  
    var shareView = new Strava.Share.ShareMenuView('.page', {
      source: 'new-activity',
      activityId: 3581188761,
      controller: shareController,
      facebookOpenGraph: true
    });
  
    var router = new Strava.Share.Router();
  
    // Facebook Share Route
    router.on('route:share:facebook', function(){
      pageView.handleOverviewClicked();
      shareView.clickShareFacebook();
      document.addEventListener('lightbox:hide', function(){
        router.navigate("activities/3581188761");
      });
    });
  
    // Embed Share Route
    router.on('route:share:embed', function(){
      pageView.handleOverviewClicked();
      shareView.openEmbedDialog();
      jQuery('#embed-share-activity-dialog').on('dialogclose', function(){
        router.navigate("activities/3581188761");
      });
    });
  });
</script>
